function Student(rollno, name, marks){
    this.rollno = rollno;
    this.name = name;
    this.marks = marks;
}
Student.prototype.total = function (){
    return this.marks.reduce((tot, mark)=>tot+mark,tot=0);
}
Student.prototype.grade = function(){
    return (this.total()/this.marks.length)>90?"A Grade":"B Grade";
}
